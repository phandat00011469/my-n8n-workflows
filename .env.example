# N8N Workflow Environment Variables
# Copy this file to .env and fill in your actual values

# Google Drive OAuth2 API Credential ID
GOOGLE_DRIVE_CREDENTIAL_ID=YOUR_GOOGLE_DRIVE_CREDENTIAL_ID

# Google Gemini API Credential ID  
GOOGLE_GEMINI_CREDENTIAL_ID=YOUR_GOOGLE_GEMINI_CREDENTIAL_ID

# Webhook ID for chat trigger
WEBHOOK_ID=YOUR_WEBHOOK_ID

# Workflow Instance ID
INSTANCE_ID=YOUR_INSTANCE_ID

# Workflow Version ID
VERSION_ID=YOUR_VERSION_ID

# Workflow ID
WORKFLOW_ID=YOUR_WORKFLOW_ID
